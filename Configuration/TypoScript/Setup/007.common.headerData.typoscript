# -------------------------------------------------------------
# I add here the call for the css and js files
#
# 
# -------------------------------------------------------------

page {
	meta {
 		viewport = {$plugin.tx_bhsiteconf.meta.viewport}
		apple-mobile-web-app-capable = {$plugin.tx_bhsiteconf.meta.apple-mobile-web-app-capable}
	}	



	includeCSS {
		all = EXT:bhsiteconf/Resources/Public/Assets/Css/app.min.css
	}
	
	includeJSFooterlibs {
    	jquery = EXT:bhsiteconf/Resources/Public/Assets/Js/Vendor/jquery.min.js
    	jquery.forceOnTop = 1
	}

	includeJSFooter {
    	all = EXT:bhsiteconf/Resources/Public/Assets/Js/app.min.js	
	}



}